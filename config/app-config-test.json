{
  "authToken": "secret-gh-token",
  "owner": "MyOrganization",
  "extractDir": "temp",
  "perPage": 100,
  "languages": [
    {
      "name": "TypeScript",
      "extension": ".ts"
    },
    {
      "name": "JavaScript",
      "extension": ".js"
    },
    {
      "name": "HTML",
      "extension": ".html"
    }
  ],
  "packageFile": "package.json",
  "dependencies": [
    "@some/npm-dep"
  ],
